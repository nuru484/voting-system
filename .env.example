# Database connection string
DATABASE_URL="postgresql://your_username:your_password@localhost:5432/your_database_name?schema=your_schema"
# Example:
# DATABASE_URL="postgresql://nurudeen:0553997465@localhost:5432/voting_system?schema=public"

# Secret key used to sign session cookies (keep this safe!)
SESSION_SECRET="your_super_secret_session_key"
# Example:
# SESSION_SECRET="C5npX8vA2qsxrn9ZCcOinNN7DK53i09X27Lf8qGyVH8Ps60feO8LJBi425FLB9aSeAkfCBUriyYR4Zn/IMQ3lQ"

# FROG SMS API credentials
FROG_API_KEY="your_frog_api_key"
FROG_SENDER_ID="your_frog_sender_id"
FROG_USERNAME="your_frog_username"
# Example:
# FROG_API_KEY="$2a$10$x43mI8vd9qsnuha07KtpL.Dr5uupG8XiU8IbzcWzRpw9tArtJMVFi"
# FROG_SENDER_ID="afatech"
# FROG_USERNAME="nuru"

# Cloudinary (image hosting) credentials
CLOUDINARY_CLOUD_NAME="your_cloud_name"
CLOUDINARY_API_KEY="your_api_key"
CLOUDINARY_API_SECRET="your_api_secret"
# Example:
# CLOUDINARY_CLOUD_NAME="dam0swaaq"
# CLOUDINARY_API_KEY="185554684825193"
# CLOUDINARY_API_SECRET="2VKNbaREyJ0y2ggJUIxT8v0suJg"
